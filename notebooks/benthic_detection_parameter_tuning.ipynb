    "def analyze_frame_range(start_frame, end_frame, dark_threshold, bright_threshold, min_area, max_area, video_path):\n",
    "    \"\"\"Analyze multiple frames and count detections\"\"\"\n",
    "    \n",
    "    all_dark_blobs = []\n",
    "    all_bright_blobs = []\n",
    "    frame_stats = []\n",
    "    \n",
    "    cap = cv2.VideoCapture(str(video_path))\n",
    "    \n",
    "    for frame_idx in range(start_frame, min(end_frame, total_frames)):\n",
    "        cap.set(cv2.CAP_PROP_POS_FRAMES, frame_idx)\n",
    "        ret, frame = cap.read()\n",
    "        \n",
    "        if not ret:\n",
    "            break\n",
    "        \n",
    "        dark_blobs, _ = detect_dark_blobs(frame, dark_threshold)\n",
    "        bright_blobs, _ = detect_bright_blobs(frame, bright_threshold)\n",
    "        \n",
    "        dark_blobs = [b for b in dark_blobs if min_area <= b['area'] <= max_area]\n",
    "        bright_blobs = [b for b in bright_blobs if min_area <= b['area'] <= max_area]\n",
    "        \n",
    "        all_dark_blobs.extend(dark_blobs)\n",
    "        all_bright_blobs.extend(bright_blobs)\n",
    "        \n",
    "        frame_stats.append({\n",
    "            'frame': frame_idx,\n",
    "            'dark_count': len(dark_blobs),\n",
    "            'bright_count': len(bright_blobs),\n",
    "            'total_count': len(dark_blobs) + len(bright_blobs)\n",
    "        })\n",
    "    \n",
    "    cap.release()\n",
    "    \n",
    "    # Plot results\n",
    "    fig, axes = plt.subplots(1, 2, figsize=(14, 4))\n",
    "    \n",
    "    frames = [s['frame'] for s in frame_stats]\n",
    "    dark_counts = [s['dark_count'] for s in frame_stats]\n",
    "    bright_counts = [s['bright_count'] for s in frame_stats]\n",
    "    total_counts = [s['total_count'] for s in frame_stats]\n",
    "    \n",
    "    # Plot counts\n",
    "    axes[0].plot(frames, dark_counts, label='Dark Blobs', marker='o', markersize=3)\n",
    "    axes[0].plot(frames, bright_counts, label='Bright Blobs', marker='s', markersize=3)\n",
    "    axes[0].plot(frames, total_counts, label='Total', marker='^', markersize=3, linewidth=2)\n",
    "    axes[0].set_xlabel('Frame')\n",
    "    axes[0].set_ylabel('Blob Count')\n",
    "    axes[0].set_title(f'Blob Detections ({start_frame}-{end_frame})')\n",
    "    axes[0].legend()\n",
    "    axes[0].grid(True, alpha=0.3)\n",
    "    \n",
    "    # Statistics\n",
    "    ax_stats = axes[1]\n",
    "    ax_stats.axis('off')\n",
    "    \n",
    "    stats_text = f\"\"\"\n",
    "    BATCH ANALYSIS RESULTS\n",
    "    ────────────────────────────────\n",
    "    Frame Range: {start_frame}-{end_frame}\n",
    "    Frames Analyzed: {len(frame_stats)}\n",
    "    \n",
    "    DARK BLOBS:\n",
    "      Total: {len(all_dark_blobs)}\n",
    "      Per Frame Avg: {len(all_dark_blobs)/len(frame_stats):.1f}\n",
    "      Max in Single Frame: {max(dark_counts) if dark_counts else 0}\n",
    "    \n",
    "    BRIGHT BLOBS:\n",
    "      Total: {len(all_bright_blobs)}\n",
    "      Per Frame Avg: {len(all_bright_blobs)/len(frame_stats):.1f}\n",
    "      Max in Single Frame: {max(bright_counts) if bright_counts else 0}\n",
    "    \n",
    "    COMBINED:\n",
    "      Total Detections: {len(all_dark_blobs) + len(all_bright_blobs)}\n",
    "      Per Frame Avg: {(len(all_dark_blobs) + len(all_bright_blobs))/len(frame_stats):.1f}\n",
    "    \"\"\"\n",
    "    \n",
    "    ax_stats.text(0.05, 0.95, stats_text, transform=ax_stats.transAxes,\n",
    "                 fontsize=10, verticalalignment='top', fontfamily='monospace',\n",
    "                 bbox=dict(boxstyle='round', facecolor='lightblue', alpha=0.5))\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "print(\"Batch analysis function loaded!\")"
   ]
