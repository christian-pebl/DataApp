    "def detect_dark_blobs(frame, dark_threshold):\n",
    "    \"\"\"Detect dark blobs in frame\"\"\"\n",
    "    gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY) if len(frame.shape) == 3 else frame\n",
    "    blurred = cv2.GaussianBlur(gray, (5, 5), 0)\n",
    "    \n",
    "    frame_float = blurred.astype(float)\n",
    "    dark_pixels = (frame_float < 128.0).astype(np.uint8) * 255\n",
    "    deviation = np.abs(frame_float - 128.0)\n",
    "    \n",
    "    _, binary_deviation = cv2.threshold(\n",
    "        deviation.astype(np.uint8),\n",
    "        dark_threshold,\n",
    "        255,\n",
    "        cv2.THRESH_BINARY\n",
    "    )\n",
    "    \n",
    "    binary = cv2.bitwise_and(dark_pixels, binary_deviation)\n",
    "    kernel = cv2.getStructuringElement(cv2.MORPH_ELLIPSE, (5, 5))\n",
    "    binary = cv2.morphologyEx(binary, cv2.MORPH_CLOSE, kernel)\n",
    "    \n",
    "    contours, _ = cv2.findContours(binary, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "    blobs = []\n",
    "    \n",
    "    for contour in contours:\n",
    "        area = cv2.contourArea(contour)\n",
    "        M = cv2.moments(contour)\n",
    "        if M['m00'] > 0:\n",
    "            cx = int(M['m10'] / M['m00'])\n",
    "            cy = int(M['m01'] / M['m00'])\n",
    "            x, y, w, h = cv2.boundingRect(contour)\n",
    "            blobs.append({\n",
    "                'bbox': (x, y, w, h),\n",
    "                'centroid': (cx, cy),\n",
    "                'area': area,\n",
    "                'type': 'dark'\n",
    "            })\n",
    "    \n",
    "    return blobs, binary\n",
    "\n",
    "def detect_bright_blobs(frame, bright_threshold):\n",
    "    \"\"\"Detect bright blobs in frame\"\"\"\n",
    "    gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY) if len(frame.shape) == 3 else frame\n",
    "    blurred = cv2.GaussianBlur(gray, (5, 5), 0)\n",
    "    \n",
    "    frame_float = blurred.astype(float)\n",
    "    bright_pixels = (frame_float > 128.0).astype(np.uint8) * 255\n",
    "    deviation = np.abs(frame_float - 128.0)\n",
    "    \n",
    "    _, binary_deviation = cv2.threshold(\n",
    "        deviation.astype(np.uint8),\n",
    "        bright_threshold,\n",
    "        255,\n",
    "        cv2.THRESH_BINARY\n",
    "    )\n",
    "    \n",
    "    binary = cv2.bitwise_and(bright_pixels, binary_deviation)\n",
    "    kernel = cv2.getStructuringElement(cv2.MORPH_ELLIPSE, (5, 5))\n",
    "    binary = cv2.morphologyEx(binary, cv2.MORPH_CLOSE, kernel)\n",
    "    \n",
    "    contours, _ = cv2.findContours(binary, cv2.RETR_EXTERNAL, cv2.CHAIN_APPROX_SIMPLE)\n",
    "    blobs = []\n",
    "    \n",
    "    for contour in contours:\n",
    "        area = cv2.contourArea(contour)\n",
    "        M = cv2.moments(contour)\n",
    "        if M['m00'] > 0:\n",
    "            cx = int(M['m10'] / M['m00'])\n",
    "            cy = int(M['m01'] / M['m00'])\n",
    "            x, y, w, h = cv2.boundingRect(contour)\n",
    "            blobs.append({\n",
    "                'bbox': (x, y, w, h),\n",
    "                'centroid': (cx, cy),\n",
    "                'area': area,\n",
    "                'type': 'bright'\n",
    "            })\n",
    "    \n",
    "    return blobs, binary\n",
    "\n",
    "print(\"Detection functions loaded!\")"
   ]
