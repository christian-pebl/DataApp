    "def analyze_frame(frame_idx, dark_threshold, bright_threshold, min_area, max_area, video_path):\n",
    "    \"\"\"Analyze single frame with current parameters\"\"\"\n",
    "    \n",
    "    # Load frame\n",
    "    cap = cv2.VideoCapture(str(video_path))\n",
    "    cap.set(cv2.CAP_PROP_POS_FRAMES, frame_idx)\n",
    "    ret, frame = cap.read()\n",
    "    cap.release()\n",
    "    \n",
    "    if not ret:\n",
    "        print(f\"Could not read frame {frame_idx}\")\n",
    "        return\n",
    "    \n",
    "    # Detect blobs\n",
    "    dark_blobs, dark_binary = detect_dark_blobs(frame, dark_threshold)\n",
    "    bright_blobs, bright_binary = detect_bright_blobs(frame, bright_threshold)\n",
    "    \n",
    "    # Filter by area\n",
    "    dark_blobs = [b for b in dark_blobs if min_area <= b['area'] <= max_area]\n",
    "    bright_blobs = [b for b in bright_blobs if min_area <= b['area'] <= max_area]\n",
    "    \n",
    "    # Create visualization\n",
    "    fig, axes = plt.subplots(2, 3, figsize=(16, 8))\n",
    "    fig.suptitle(f'Frame {frame_idx} - Detection Analysis (Dark: {dark_threshold}, Bright: {bright_threshold}, Min Area: {min_area})', fontsize=14)\n",
    "    \n",
    "    # Original frame\n",
    "    frame_rgb = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB)\n",
    "    axes[0, 0].imshow(frame_rgb)\n",
    "    axes[0, 0].set_title('Original Frame')\n",
    "    axes[0, 0].axis('off')\n",
    "    \n",
    "    # Dark blobs\n",
    "    frame_dark = frame_rgb.copy()\n",
    "    for blob in dark_blobs:\n",
    "        x, y, w, h = blob['bbox']\n",
    "        cv2.rectangle(frame_dark, (x, y), (x+w, y+h), (0, 255, 0), 2)\n",
    "    axes[0, 1].imshow(frame_dark)\n",
    "    axes[0, 1].set_title(f'Dark Blobs Detected: {len(dark_blobs)}')\n",
    "    axes[0, 1].axis('off')\n",
    "    \n",
    "    # Bright blobs\n",
    "    frame_bright = frame_rgb.copy()\n",
    "    for blob in bright_blobs:\n",
    "        x, y, w, h = blob['bbox']\n",
    "        cv2.rectangle(frame_bright, (x, y), (x+w, y+h), (255, 0, 0), 2)\n",
    "    axes[0, 2].imshow(frame_bright)\n",
    "    axes[0, 2].set_title(f'Bright Blobs Detected: {len(bright_blobs)}')\n",
    "    axes[0, 2].axis('off')\n",
    "    \n",
    "    # Dark binary\n",
    "    axes[1, 0].imshow(dark_binary, cmap='gray')\n",
    "    axes[1, 0].set_title('Dark Binary Mask')\n",
    "    axes[1, 0].axis('off')\n",
    "    \n",
    "    # Bright binary\n",
    "    axes[1, 1].imshow(bright_binary, cmap='gray')\n",
    "    axes[1, 1].set_title('Bright Binary Mask')\n",
    "    axes[1, 1].axis('off')\n",
    "    \n",
    "    # Statistics\n",
    "    ax_stats = axes[1, 2]\n",
    "    ax_stats.axis('off')\n",
    "    \n",
    "    stats_text = f\"\"\"\n",
    "    DETECTION STATISTICS\n",
    "    ────────────────────────\n",
    "    Dark Blobs: {len(dark_blobs)}\n",
    "    Bright Blobs: {len(bright_blobs)}\n",
    "    Total Blobs: {len(dark_blobs) + len(bright_blobs)}\n",
    "    \n",
    "    Dark Blob Areas:\n",
    "      Min: {min([b['area'] for b in dark_blobs]) if dark_blobs else 'N/A'}\n",
    "      Max: {max([b['area'] for b in dark_blobs]) if dark_blobs else 'N/A'}\n",
    "      Avg: {np.mean([b['area'] for b in dark_blobs]):.1f if dark_blobs else 'N/A'}\n",
n",
    "    Bright Blob Areas:\n",
    "      Min: {min([b['area'] for b in bright_blobs]) if bright_blobs else 'N/A'}\n",
    "      Max: {max([b['area'] for b in bright_blobs]) if bright_blobs else 'N/A'}\n",
    "      Avg: {np.mean([b['area'] for b in bright_blobs]):.1f if bright_blobs else 'N/A'}\n",
    "    \"\"\"\n",
    "    \n",
    "    ax_stats.text(0.05, 0.95, stats_text, transform=ax_stats.transAxes, \n",
    "                 fontsize=10, verticalalignment='top', fontfamily='monospace',\n",
    "                 bbox=dict(boxstyle='round', facecolor='wheat', alpha=0.5))\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "print(\"Analysis function loaded!\")"
   ]
