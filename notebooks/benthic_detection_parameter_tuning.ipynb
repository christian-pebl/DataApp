    "def export_parameters():\n",
    "    \"\"\"Export current parameters as Python code\"\"\"\n",
    "    \n",
    "    params = {\n",
    "        'detection': {\n",
    "            'dark_threshold': dark_threshold_slider.value,\n",
    "            'bright_threshold': bright_threshold_slider.value,\n",
    "            'min_area': min_area_slider.value,\n",
    "            'max_area': max_area_slider.value,\n",
    "            'min_circularity': min_circularity_slider.value,\n",
    "            'max_aspect_ratio': max_aspect_ratio_slider.value,\n",
    "        },\n",
    "        'tracking': {\n",
    "            'max_distance': max_distance_slider.value,\n",
    "            'max_skip_frames': max_skip_frames_slider.value,\n",
    "        },\n",
    "        'validation': {\n",
    "            'min_track_length': min_track_length_slider.value,\n",
    "            'min_displacement': min_displacement_slider.value,\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    print(\"\\n\" + \"=\"*60)\n",
    "    print(\"CURRENT PARAMETERS\")\n",
    "    print(\"=\"*60)\n",
    "    print(json.dumps(params, indent=2))\n",
